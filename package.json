  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.19",
    "glob": "^10.3.10",
    "reflect-metadata": "^0.2.1",
    "rollup": "^4.12.0",
    "rollup-plugin-ts": "^3.4.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0"
